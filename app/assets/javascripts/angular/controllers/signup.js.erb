app.controller('SignUpCtrl', ['$scope', '$auth', '$state', function($scope, $auth, $state){
  $scope.signup = function () {
    $auth.submitRegistration($scope.registrationForm)
    .then(function(resp) {
      console.log(resp);

      $state.go('places');
    })
    .catch(function(resp) {
      console.log(resp.data.errors.full_messages);
    });
  };
}])