app.controller('LogInCtrl', ['$scope', '$auth', '$state', function($scope, $auth, $state){
  $scope.login = function () {
    $auth.submitLogin($scope.loginForm)
    .then(function(resp) {
      console.log(resp);

      $state.go('places');
    })
    .catch(function(resp) {
      console.log(resp.errors);
    });
  };
}])