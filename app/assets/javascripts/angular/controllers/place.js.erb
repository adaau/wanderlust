app.controller('PlaceCtrl', ['$scope', '$auth', '$http', function($scope, $auth, $http){
  console.log('PlaceCtrl')
  $scope.places = [];

  var url = "<%= ENV['URL'] %>" || "http://localhost:3000";

  function getPlaces() {
    $http
      .get(url + '/api/places')
      .then(function(response){
        $scope.places = response.data;
      });
  }

  getPlaces();
}])
